{
  "name": "llm-sv-tabs",
  "version": "1.0.0",
  "description": "LLM-powered browser with Electron and Svelte",
  "main": "src/main/main.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "electron:dev": "electron .",
    "start": "npm run build && electron .",
